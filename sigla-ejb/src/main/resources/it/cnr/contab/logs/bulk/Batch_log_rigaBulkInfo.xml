<?xml version="1.0" encoding="UTF-8"?>
<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="https://contab.cnr.it/schema/SIGLA" 
		xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA https://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">
	<fieldProperty
		name="ti_messaggio"
		property="ti_messaggio"
		maxLength="5"
		inputSize="5"
		inputType="TEXT"
		label="Stato"
		caseSensitiveSearch="false" />
	<fieldProperty
		name="messaggio"
		property="messaggio"
		cols="100"
		rows="5"
		inputType="TEXTAREA"
		label="Programma"
		caseSensitiveSearch="false" />
	<fieldProperty
		name="pg_esecuzione"
		property="pg_esecuzione"
		inputType="TEXT"
		label="Progressivo esecuzione" />
	<fieldProperty
		name="trace"
		property="trace"
		cols="100"
		rows="5"
		inputType="TEXTAREA"
		label="Traccia"
		caseSensitiveSearch="false" />
	<fieldProperty
		name="pg_riga"
		property="pg_riga"
		inputType="TEXT"
		label="Progressivo di riga" />
	<fieldProperty
		name="note"
		property="note"
		cols="100"
		rows="5"
		inputType="TEXTAREA"
		label="Note"
		caseSensitiveSearch="false" />

	<formFieldProperty
		name="pg_esecuzione" />
