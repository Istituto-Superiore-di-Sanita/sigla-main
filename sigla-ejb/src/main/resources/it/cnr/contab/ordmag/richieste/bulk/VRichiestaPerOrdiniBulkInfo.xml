	shortDescription="Riga richieste"
	longDescription="Riga richieste">
	<fieldProperty
		name="cdCds"
		property="cdCds"
		inputType="TEXT"
		inputSize="30"
		maxLength="30"
		enabledOnEdit="false"
		enabledOnSearch="false"
		nullable="false"
		label="cdCds" />
	<fieldProperty
		name="cdUnitaOperativa"
