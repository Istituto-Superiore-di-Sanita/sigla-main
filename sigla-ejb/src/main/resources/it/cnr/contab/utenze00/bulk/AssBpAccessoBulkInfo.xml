            name="businessProcess"
            property="businessProcess"
            inputType="TEXTAREA"
            cols="60"
            rows="5"
            maxLength="200"
            enabledOnEdit="false"
            enabledOnSearch="false"
            nullable="false"
            label="businessProcess"/>
    <fieldProperty
            name="tiFunzione"
            property="tiFunzione"
            inputType="TEXT"
            inputSize="1"
            maxLength="1"
            enabledOnEdit="false"
            label="tiFunzione"/>
    <fieldProperty
            name="esercizioInizioValidita"
            property="esercizioInizioValidita"
            inputType="TEXT"
            inputSize="4"
            maxLength="4"
            enabledOnEdit="false"
            label="esercizioInizioValidita"/>
    <fieldProperty
            name="esercizioFineValidita"
            property="esercizioFineValidita"
            inputType="TEXT"
            inputSize="4"
            maxLength="4"
            enabledOnEdit="false"
            label="esercizioFineValidita"/>
    <form
            name="searchtool">
        <formFieldProperty
                name="cdAccesso"/>
        <formFieldProperty
                name="businessProcess"/>
    </form>
    <form
            name="accesso">
        <formFieldProperty
                name="cdAccesso"/>
        <formFieldProperty
                name="dsAccesso"/>
    </form>
    <formFieldProperty
            name="cdAccesso"/>
    <formFieldProperty
            name="dsAccesso"/>
    <formFieldProperty
            name="businessProcess"/>
    <formFieldProperty
            name="tiFunzione"/>
    <formFieldProperty
            name="esercizioInizioValidita"/>
    <formFieldProperty
            name="esercizioFineValidita"/>
    <findFieldProperty
            name="cdAccesso"/>
    <findFieldProperty
            name="businessProcess"/>
    <findFieldProperty
            name="tiFunzione"/>
    <findFieldProperty
            name="esercizioInizioValidita"/>
    <findFieldProperty
            name="esercizioFineValidita"/>
    <columnFieldProperty
            name="cdAccesso"/>
    <columnFieldProperty
            name="businessProcess"/>
    <columnFieldProperty
            name="tiFunzione"/>
    <columnFieldProperty
            name="esercizioInizioValidita"/>
    <columnFieldProperty
            name="esercizioFineValidita"/>

    <columnSet name="accesso">
        <columnFieldProperty name="cdAccesso"/>
        <columnFieldProperty name="dsAccesso"/>
    </columnSet>
    <freeSearchSet name="accesso">
        <findFieldProperty name="cdAccesso"/>
        <findFieldProperty name="dsAccesso"/>
    </freeSearchSet>
