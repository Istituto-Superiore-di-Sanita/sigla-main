		inputType="ROTEXT"
		formatName="it.cnr.contab.util.EuroFormat"
		label="Importo Bonus" />
	<fieldProperty
		name="ds_condizione"
		property="bonus_condizioni.ds_condizione"
	    maxLength="100"
	    inputSize="100"
	    inputType="TEXT"
		enabledOnInsert="false"
		enabledOnEdit="false" 
		label="Condizione" />
	<fieldProperty
		name="cd_condizione"
		property="bonus_condizioni.cd_condizione"
		enabledOnInsert="true"
		enabledOnEdit="false" 
		label="Condizione" />
	<fieldProperty
		name="dt_richiesta"
		property="dt_richiesta"
		inputType="TEXT"
		readonlyProperty="ROBonus"
		formatName="date_short"
		label="Data Richiesta" />
	<fieldProperty
