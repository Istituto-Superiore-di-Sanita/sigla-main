	<fieldProperty
			name="tipoPosizione"
			property="tipoPosizione"
			inputType="TEXT"
			inputSize="10"
			maxLength="3"
			enabledOnEdit="false"
			nullable="false"
			keysProperty="tipoPosizioneKeys"
			label="Posizione" />
