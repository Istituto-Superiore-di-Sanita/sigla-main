		name="pg_obbligazione" />
	<formFieldProperty
		name="motivazione" />		

	<findFieldProperty
		name="cd_cds" />
	<findFieldProperty
		name="esercizio" />
	<findFieldProperty
		name="pg_modifica" />
	<findFieldProperty
		name="ds_modifica" />
	<findFieldProperty
		name="dt_modifica" />
	<findFieldProperty
		name="esercizio_originale" />
	<findFieldProperty
		name="pg_obbligazione" />
	<findFieldProperty
		name="motivazione" />		

	<columnFieldProperty
		name="cd_cds" />
	<columnFieldProperty
		name="esercizio" />
	<columnFieldProperty
		name="pg_modifica" />
	<columnFieldProperty
		name="ds_modifica" />
	<columnFieldProperty
		name="dt_modifica" />
	<columnFieldProperty
		name="esercizio_originale" />
