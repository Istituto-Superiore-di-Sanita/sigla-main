		/>
	<fieldProperty
		name="tipo"
		property="tipo"
		label="Tipo"
		keysProperty="tipo_naturaKeys"
		inputType="SELECT"
		nullable="false"
		/>
	<columnFieldProperty
		name="cd_natura"
		/>
	<columnFieldProperty
		name="ds_natura"
		/>
	<columnFieldProperty
		name="tipo"
		/>

	<formFieldProperty
		name="cd_natura"
		/>
	<formFieldProperty
		name="ds_natura"
		/>
	<formFieldProperty
		name="tipo"
		/>

	<findFieldProperty
		name="cd_natura"
		/>
	<findFieldProperty
		name="ds_natura"
		/>
	<findFieldProperty
		name="tipo"
		/>
		
	<form
		name="find_natura" >
		<formFieldProperty
			name="cd_natura"
			inputSize="2"/>
		<formFieldProperty
			name="ds_natura"
			inputSize="30"/>
	</form>


</bulkInfo>