<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="https://contab.cnr.it/schema/SIGLA" 
		xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA https://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">

	<!-- ************************** FIELD PROPERTY ************************ -->

	
	<fieldProperty
		name="importo"
		property="importo"
		maxLength="24"
		inputSize="24"
		inputType="ROTEXT"
		formatName="it.cnr.contab.util.Importo2CifreFormat"
		label="Totale"/>
	<fieldProperty
		name="im_tot_iva"
		property="im_tot_iva"
		maxLength="24"
		inputSize="24"
		inputType="ROTEXT"
		formatName="it.cnr.contab.util.Importo2CifreFormat"
		label="Tot. iva"/>
	<fieldProperty
		name="im_totale"
		property="im_totale"
		maxLength="24"
		inputSize="24"
		inputType="ROTEXT"
		formatName="it.cnr.contab.util.Importo2CifreFormat"
		label="Totale"/>
			
	<!-- ************************ FORM FIELD PROPERTY ******************** -->


	<formFieldProperty name="importo" />
	<formFieldProperty name="im_tot_iva" />
	<formFieldProperty name="im_totale" />
	
	
	<!-- ************************ FIND FIELD PROPERTY ******************** -->

	<findFieldProperty name="importo" />
	<findFieldProperty name="im_tot_iva" />
	<findFieldProperty name="im_totale" />


</bulkInfo>