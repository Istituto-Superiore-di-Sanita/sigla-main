<?xml version="1.0" encoding="UTF-8"?>
<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="https://contab.cnr.it/schema/SIGLA" 
		xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA https://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">
	
	<fieldProperty
		name="findUoForPrint"
		property="uoForPrint"
		inputType="SEARCHTOOL"
		completeOnSave = "true"
		label="Unita Organizzativa" />
	<fieldProperty
		name="cdUoForPrint"
		property="uoForPrint.cd_unita_organizzativa"
		maxLength="30"
		inputSize="20"
		inputType="TEXT"
		nullable="false"
		readonlyProperty="ROUoForPrint"
		label="Codice UO" />
	<fieldProperty
		name="dsUoForPrint"
		property="uoForPrint.ds_unita_organizzativa"
		maxLength="80"
		inputSize="60"
		inputType="ROTEXT"
		label="Descrizione UO" />
	<fieldProperty
		name="pgInizio"
		property="pgInizio"
		inputType="TEXT"
		maxLength="10"
		inputSize="10"
		nullable="false"
		label="Numero Inizio" />
	<fieldProperty
		name="pgFine"
		property="pgFine"
		inputType="TEXT"
		maxLength="10"
		inputSize="10"
		nullable="false"
		label="Numero Fine" />
		
	<fieldProperty
		name="cdUOCRForPrint"
		property="cdUOCRForPrint"/>
	
	<formFieldProperty
		name="esercizio"
		inputType="ROTEXT" />
	<formFieldProperty
		name="cd_cds"
		inputType="ROTEXT" />
	
	<formFieldProperty name="findUoForPrint" />
	<formFieldProperty name="cdUoForPrint" />
	<formFieldProperty name="dsUoForPrint" />
	<formFieldProperty name="pgInizio" />
	<formFieldProperty name="pgFine" />
	
	<formFieldProperty name="cdUOCRForPrint" />

	<form
		name="/cnrdoccont/doccont/stampaordine.rpt">
		<formFieldProperty
			name="esercizio"
			inputType="ROTEXT" />
		<formFieldProperty 
			name="cd_cds"
			inputType="ROTEXT" />
		<formFieldProperty name="findUoForPrint" />
		<formFieldProperty name="cdUoForPrint" />
		<formFieldProperty name="dsUoForPrint" />
		<formFieldProperty name="pgInizio" />
		<formFieldProperty name="pgFine" />
	</form>

	<printForm
		name="/cnrdoccont/doccont/stampaordine.rpt">
		<printFieldProperty 
			name="esercizio"
			parameterPosition="0"
			parameterType="STRING" />
		<printFieldProperty 
			name="cd_cds"
			parameterPosition="1"
			parameterType="STRING" />
		<printFieldProperty 
			name="cdUOCRForPrint"
			parameterPosition="2"
			parameterType="STRING" />
		<printFieldProperty 
			name="pgInizio"
			parameterPosition="3"
			parameterType="STRING" />
		<printFieldProperty
			name="pgFine"
			parameterPosition="4"
			parameterType="STRING" />
	</printForm>

	<printForm
		name="/cnrdoccont/doccont/stampaordine.jasper">
		<printFieldProperty 
			name="esercizio"
			parameterPosition="0"
			parameterType="Integer" 
			paramNameJR="Esercizio"
			paramTypeJR="java.lang.Integer"/>
		<printFieldProperty 
			name="cd_cds"
			parameterPosition="1"
			parameterType="STRING" 
			paramNameJR="CDS"
			paramTypeJR="java.lang.String"/>
		<printFieldProperty 
			name="cdUOCRForPrint"
			parameterPosition="2"
			parameterType="STRING" 
			paramNameJR="UO"
			paramTypeJR="java.lang.String"/>
		<printFieldProperty 
			name="pgInizio"
			parameterPosition="3"
			parameterType="INTEGER" 
			paramNameJR="Numero_da"
			paramTypeJR="java.lang.Integer"/>
		<printFieldProperty
			name="pgFine"
			parameterPosition="4"
			parameterType="INTEGER" 
			paramNameJR="Numero_a"
			paramTypeJR="java.lang.Integer"/>
	</printForm>

</bulkInfo>