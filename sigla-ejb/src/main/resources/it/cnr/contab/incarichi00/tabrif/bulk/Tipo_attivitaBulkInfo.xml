<?xml version="1.0" encoding="iso-8859-1"?>
<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="https://contab.cnr.it/schema/SIGLA" 
		xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA https://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">
	<fieldProperty
		name="cd_tipo_attivita"
		property="cd_tipo_attivita"
		inputType="TEXT"
		inputSize="5"
		maxLength="5"
		enabledOnSearch="true"
		nullable="false"
		label="Codice" />
	<fieldProperty
		name="ds_tipo_attivita"
		property="ds_tipo_attivita"
		inputType="TEXTAREA"
		maxLength="200"
		cols="60"
		rows="5"
		enabledOnSearch="true"
		nullable="false"
		label="Descrizione" />
	<fieldProperty
		name="fl_pubblica_contratto"
		property="fl_pubblica_contratto"
		inputType="CHECKBOX"
		enabledOnSearch="true"
		enabledOnInsert="false"
		enabledOnEdit="false"
		nullable="false"
		label="Pubblica contratti stipulati" />
	<fieldProperty
		name="fl_cancellato"
		property="fl_cancellato"
		inputType="CHECKBOX"
		enabledOnSearch="true"
		enabledOnInsert="false"
		enabledOnEdit="false"
		nullable="false"
