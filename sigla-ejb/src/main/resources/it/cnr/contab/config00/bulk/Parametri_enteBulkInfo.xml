	<columnFieldProperty
		name="fl_autenticazione_ldap" />
	<columnFieldProperty
		name="ldap_user" />						
	<columnFieldProperty
		name="ldap_password" />						
	<columnFieldProperty
		name="ldap_base_dn" />						
	<columnFieldProperty
		name="ldap_app_name" />						
	<columnFieldProperty
		name="dt_ldap_migrazione" />						
	<columnFieldProperty
		name="ldap_link_cambio_password" />						
