	<fieldProperty
		name="elemento_voce"
		property="elemento_voce"
		inputType="SEARCHTOOL"
		formName="searchtool"
		enabledOnEdit="false"
		enabledOnInsert="true"
		label="Voce di Bilancio" />
	<fieldProperty
		name="cd_elemento_voce"
		property="elemento_voce.cd_elemento_voce"
		maxLength="10"
		inputSize="10"
		inputType="ROTEXT"		
		label="Codice" />
	<fieldProperty
		name="ds_elemento_voce"
		property="elemento_voce.ds_elemento_voce"
		maxLength="100"
		inputSize="100"
		inputType="ROTEXT"
		label="Descrizione"/>

	<formFieldProperty name="elemento_voce" />
