		inputSize="20"
		enabledOnSearch="true"
		/>
		
	<columnFieldProperty
		name="cd_natura"
		/>
	<columnFieldProperty
		name="cd_elemento_voce"
		/>
	
	<formFieldProperty
		name="cd_natura"
		/>
	<formFieldProperty
		name="find_elemento_voce"
		/>
	<formFieldProperty
		name="cd_elemento_voce"
		/>
