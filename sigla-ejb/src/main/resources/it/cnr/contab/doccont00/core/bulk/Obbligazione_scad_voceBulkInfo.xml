		/>
	<findFieldProperty
		name="cd_voce"
		/>
	<findFieldProperty
		name="esercizio"
		/>
	<findFieldProperty
		name="im_voce"
		/>
	<findFieldProperty
		name="esercizio_originale"
		/>
	<findFieldProperty
		name="pg_obbligazione"
		/>
	<findFieldProperty
		name="pg_obbligazione_scadenzario"
		/>
	<findFieldProperty
