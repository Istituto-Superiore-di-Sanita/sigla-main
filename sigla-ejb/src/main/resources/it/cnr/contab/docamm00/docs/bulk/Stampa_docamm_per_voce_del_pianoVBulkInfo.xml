<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="https://contab.cnr.it/schema/SIGLA" 
		xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA https://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">


	<formFieldProperty
		name="esercizio"
		property="esercizio"
		inputType="ROTEXT"
		label="Esercizio" />
	<formFieldProperty
		name="cd_cds"
		property="cd_cds"
		inputType="ROTEXT" 
		label="CDS" />

	<fieldProperty
		name="findUOForPrint"
		property="uoForPrint"
		inputType="SEARCHTOOL"
		formName="searchtool"
		completeOnSave="true"
		nullable="false"
		label="Unita Organizzativa" />
	<fieldProperty
		name="cdUOCRForPrint"
		property="cdUOCRForPrint"/>
		
	<fieldProperty
		name="findVoceDPForPrint"
		property="vocedpForPrint"
		inputType="SEARCHTOOL"
		formName="searchtool"
		completeOnSave="true"
		enabledOnSearch="true"
		label="Voce del Piano" />
	<fieldProperty
		name="voceDPForPrint"
		property="voceDPForPrint"/>
	
	<fieldProperty
		name="findTerzo"
		property="terzo"
		inputType="SEARCHTOOL"
		completeOnSave="true"
		label="Terzo" />
	<fieldProperty
		name="cd_terzo"
		property="terzo.cd_terzo"
		maxLength="30"
		inputSize="20"
		inputType="TEXT"
		readonlyProperty="ROTerzo"
		label="Codice Terzo" />
	<fieldProperty
		name="ds_terzo"
		property="terzo.denominazione_sede"
		maxLength="80"
		inputSize="60"
		inputType="ROTEXT"
		label="Descrizione Terzo" />

	<fieldProperty
		name="cdTerzoForPrint"
		property="cdTerzoForPrint"/>
		
		
	<formFieldProperty name="esercizio" />
	<formFieldProperty name="cd_cds" />

	<formFieldProperty name="findUOForPrint" />
	<formFieldProperty name="cdUOCRForPrint" />

	<formFieldProperty name="findVoceDPForPrint" />
	<formFieldProperty name="voceDPForPrint" />
		
	<formFieldProperty name="findTerzo" />
	<formFieldProperty name="cd_terzo" />
	<formFieldProperty name="ds_terzo" />	
</bulkInfo>