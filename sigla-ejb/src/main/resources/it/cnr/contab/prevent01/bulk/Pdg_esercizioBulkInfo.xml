<?xml version="1.0" encoding="iso-8859-1"?>
<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="http://contab.cnr.it/schema/SIGLA" 
		xsi:schemaLocation="http://contab.cnr.it/schema/SIGLA http://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">
	<fieldProperty
		name="esercizio"
		property="esercizio"
		inputType="TEXT"
		inputSize="4"
		maxLength="4"
		label="Esercizio di riferimento" />
	<fieldProperty
		name="cd_centro_responsabilita"
		property="cdr.cd_centro_responsabilita"
		inputType="TEXT"
		enabledOnEdit="false"
		enabledOnFreeSearch="true"
		enabledOnSearch="true"
		label="Centro di responsabilità" />
	<fieldProperty
		name="ds_cdr"
		property="cdr.ds_cdr"
		inputType="ROTEXT"
		inputSize="50"
		enabledOnEdit="true"
		enabledOnFreeSearch="true"
		enabledOnSearch="true"
		label="Descrizione Centro di responsabilità" />
		
	
	<fieldProperty
		name="cdr"
		property="cdr"
		enabledOnEdit="true"
		enabledOnFreeSearch="true"
		enabledOnSearch="true"
		inputType="SEARCHTOOL"
		formName="searchtool"
		label="Centro di responsabilità" />

	<fieldProperty
		name="stato"
		property="stato"
		maxLength="2"
		inputSize="2"
		inputType="RADIOGROUP"
		label="Stato del CdR"
		keysProperty="statoKeys"
		enabledOnSearch="true"
		caseSensitiveSearch="false" />
	<formFieldProperty
		name="esercizio" />
	<formFieldProperty
		name="cd_centro_responsabilita" />
	<formFieldProperty
		name="ds_cdr" />
	<formFieldProperty
		name="stato"
		readonlyProperty="ROStato"
		 />
	<findFieldProperty
		name="esercizio" />
	<findFieldProperty
		name="cd_centro_responsabilita" />
	<findFieldProperty
		name="ds_cdr" />
	<findFieldProperty
		name="stato" />
	<columnFieldProperty
		name="cd_centro_responsabilita" />
	<columnFieldProperty
		name="ds_cdr" />
	<columnFieldProperty
		name="stato" />
</bulkInfo>