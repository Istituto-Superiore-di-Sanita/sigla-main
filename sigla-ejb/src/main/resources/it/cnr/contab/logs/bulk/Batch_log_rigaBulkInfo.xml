	<columnSet name="LIQUID_CORI_MASSA_COLUMN_SET">
		<columnFieldProperty
			name="pg_riga" 
			label = "Riga"/>
		<columnFieldProperty
			name="ti_messaggio" />
		<columnFieldProperty
			name="messaggio" 
			label = "U.O."/>	
		<columnFieldProperty
			name="trace" 
			label = "Errore"/>			
	</columnSet>
	
	<columnSet name="ELAB_FILE_STIP_COLUMN_SET">
		<columnFieldProperty
			name="pg_riga" 
			label = "Riga"/>
		<columnFieldProperty
			name="ti_messaggio" />
		<columnFieldProperty
			name="trace" 
			label = "Messaggio"/>			
	</columnSet>

	<findFieldProperty
		name="pg_esecuzione" />
	<findFieldProperty
		name="pg_riga" />
	<findFieldProperty
		name="ti_messaggio" />
	<findFieldProperty
		name="note" />
	<findFieldProperty
		name="messaggio" />
	<findFieldProperty
		name="trace" />

