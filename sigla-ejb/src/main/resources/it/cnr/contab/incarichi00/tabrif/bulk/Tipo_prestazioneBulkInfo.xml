		name="ds_tipo_prestazione" />
	<formFieldProperty
		name="tipo_classificazione" />
	<formFieldProperty
		name="fl_cancellato" />

	<columnFieldProperty
		name="cd_tipo_prestazione" />
	<columnFieldProperty
		name="ds_tipo_prestazione" />
	<columnFieldProperty
		name="tipo_classificazione" />
	<columnFieldProperty
		name="fl_cancellato" />

	<findFieldProperty
		name="cd_tipo_prestazione" />
	<findFieldProperty
		name="ds_tipo_prestazione" />
	<findFieldProperty
		name="tipo_classificazione" />
	<findFieldProperty
		name="fl_cancellato" />

	<form
		name="searchtool" >
		<formFieldProperty
			name="cd_tipo_prestazione" />
		<formFieldProperty
			name="ds_tipo_prestazione"
			inputType="TEXT"
			inputSize="30" />
	</form>
</bulkInfo>
