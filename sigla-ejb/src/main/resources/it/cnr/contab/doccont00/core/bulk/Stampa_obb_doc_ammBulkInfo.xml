<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="https://contab.cnr.it/schema/SIGLA" 
		xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA https://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">
	<fieldProperty
		name="esercizio"
		property="esercizio"
		inputType="ROTEXT"
		inputSize="2"
		nullable="false"
		label="Esercizio" />
	<fieldProperty
		name="findCdsForPrint"
		property="cdsForPrint"
		readonlyProperty="cdsForPrintEnabled"
		inputType="SEARCHTOOL"
		formName="searchtool"
		nullable="false"
		label="Cds" />
	<fieldProperty
		name="cdCdsForPrint"
		property="cdCdsForPrint" />
	<fieldProperty
		name="findUoForPrint"
		property="uoForPrint"
		inputType="SEARCHTOOL"
		formName="searchtoolUO"
		label="Unita Organizzativa" />
	<fieldProperty
		name="cdUoForPrint"
		property="cdUoForPrint" />
	<fieldProperty
		name="findElementoVoceForPrint"
		property="elementoVoceForPrint"
		inputType="SEARCHTOOL"
		formName="searchtool"
		label="Elemento voce" />
	<fieldProperty
		name="cdElementoVoceForPrint"
		property="cdElementoVoceForPrint" />
	<fieldProperty
		name="findEsercizioDocForPrint"
		property="esercizioDocForPrint"
		inputType="SEARCHTOOL"
		formName="searchtool"
		completeOnSave="true"
		label="Esercizio Documento"/>
	<fieldProperty
		name="cdEsercizioDocForPrint"
		property="cdEsercizioDocForPrint" />
