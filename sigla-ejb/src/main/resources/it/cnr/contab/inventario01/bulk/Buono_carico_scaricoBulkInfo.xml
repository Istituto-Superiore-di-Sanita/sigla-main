<?xml version="1.0" encoding="iso-8859-1"?>
<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="https://contab.cnr.it/schema/SIGLA" 
		xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA https://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">
	<fieldProperty
		name="pg_inventario"
		property="inventario.pg_inventario"
		inputType="ROTEXT"
		maxLength="10"
		inputSize="10"
		label="Inventario" />	
	<fieldProperty
		name="ti_documento"
		property="ti_documento"
		maxLength="1"
		inputSize="1"
		inputType="ROTEXT"
		label="Tipo Documento" />
	<fieldProperty
		name="esercizio"
		property="esercizio"
		inputType="TEXT"
		inputSize="4"
		maxLength="4"
		enabledOnFreeSearch = "false"
		enabledOnInsert = "false"
		enabledOnEdit = "false"
		enabledOnSearch = "false"
		label="Esercizio" />
	<fieldProperty
		name="pg_buono_c_s"
		property="pg_buono_c_s"
		inputType="TEXT"
		inputSize="10"
		maxLength="10"
		enabledOnFreeSearch = "true"
		enabledOnInsert = "false"
		enabledOnEdit = "false"
		enabledOnSearch = "true"		
		label="Numero Buono" />
	<fieldProperty
		name="ds_buono_carico_scarico"
		property="ds_buono_carico_scarico"
		inputType="TEXT"
		maxLength="100"
		inputSize="100"
		label="Descrizione"
		nullable="false"
		caseSensitiveSearch="false"/>
	<fieldProperty
		name="data_registrazione"
		property="data_registrazione"
		inputType="TEXT"
		maxLength="10"
		inputSize="10"
		nullable="false"
		formatName="date_short"
		label ="Data" 
		command ="doOnData_registrazioneChange"/>

	<fieldProperty
		name="cd_tipo_carico_scarico"
		property="tipoMovimento.cd_tipo_carico_scarico"
		inputType="TEXT"
		inputSize="10"
		maxLength="10"
		label="Codice del movimento di inventario" />
	<fieldProperty
		name="provenienza"
		property="provenienza"
		maxLength="100"
		inputSize="100"
		inputType="TEXT"
		label="Inventario di Provenienza"
		caseSensitiveSearch="false" />
	<fieldProperty
		name="nr_inventario"
		property="nr_inventario"
		maxLength="10"
		inputSize="10"
		inputType="TEXT"
		label="Codice Bene"/>
	<fieldProperty
		name="cd_barre"
		property="cd_barre"
		maxLength="6"
		inputSize="6"
		inputType="TEXT"
		label="Cod. barre"/>
		
	<fieldProperty
		name="tipoMovimento"
		property="tipoMovimento"
		printProperty="ds_tipo_carico_scarico"
		optionsProperty="tipoMovimenti"
		maxLength="100"
		inputSize="100"
		inputType="SELECT"
		nullable="false"
		enabledOnSearch="true"
		label="Tipo Movimento"/>
		

	/*******Dati relativi al Consegnatario dell'Inventario *****/	
	<fieldProperty
		name="cd_consegnatario"
		property="consegnatario.cd_terzo"
		inputType="ROTEXT"
		label="Consegnatario"
		maxLength="6"
		inputSize="6"
		enabledOnSearch="true"
		enabledOnEdit="true"
		caseSensitiveSearch="false" />
	<fieldProperty
		name="cognome_consegnatario"
		property="consegnatario.denominazione_sede"
		inputType="ROTEXT"
		maxLength="30"
		inputSize="30"
		enabledOnSearch="true"
		enabledOnEdit="true"
		caseSensitiveSearch="false" />
		
	/*******Dati relativi al Delegato dell'Inventario *****/	
	<fieldProperty
		name="cd_delegato"
		property="delegato.cd_terzo"
		inputType="ROTEXT"
		label="Delegato"
		maxLength="6"
		inputSize="6"
		enabledOnSearch="true"
		enabledOnEdit="true"
		caseSensitiveSearch="false" />
	<fieldProperty
		name="cognome_delegato"
		property="delegato.denominazione_sede"
		inputType="ROTEXT"
		maxLength="30"
		inputSize="30"
		enabledOnSearch="true"
		enabledOnEdit="true"
		caseSensitiveSearch="false" />
		
	/*******Dati relativi alla UO Consegntaria dell'Inventario *****/	
	<fieldProperty
		name="cd_uo_consegnataria"
		property="uo_consegnataria.cd_unita_organizzativa"
		inputType="ROTEXT"
		label="UO Consegnataria"
		maxLength="6"
		inputSize="6"
		enabledOnSearch="true"
		enabledOnEdit="true"
		caseSensitiveSearch="false" />
	<fieldProperty
		name="ds_uo_consegnataria"
		property="uo_consegnataria.ds_unita_organizzativa"
		inputType="ROTEXT"
		maxLength="30"
		inputSize="30"
		enabledOnSearch="true"
		enabledOnEdit="true"
		caseSensitiveSearch="false" />
	/************************************************/				

