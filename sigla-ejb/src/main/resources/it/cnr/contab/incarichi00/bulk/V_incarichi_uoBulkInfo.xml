<?xml version="1.0" encoding="iso-8859-1"?>
<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="http://contab.cnr.it/schema/SIGLA" 
		xsi:schemaLocation="http://contab.cnr.it/schema/SIGLA http://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">
	<fieldProperty
		name="esercizio_limite"
		property="esercizio_limite"
		inputType="TEXT"
		inputSize="4"
		maxLength="4"
		label="Esercizio" />
	<fieldProperty
		name="cd_cds"
		property="cd_cds"
		inputType="TEXT"
		inputSize="30"
		maxLength="30"
		label="Cds" />
	<fieldProperty
		name="cd_unita_organizzativa"
		property="cd_unita_organizzativa"
		inputType="TEXT"
		inputSize="10"
		maxLength="30"
		label="Unità Organizzativa" />
	<fieldProperty
		name="ds_unita_organizzativa"
		property="ds_unita_organizzativa"
		inputType="TEXT"
		inputSize="50"
		maxLength="300"
		label="Descrizione" />
	<fieldProperty
		name="cd_tipo_limite"
		property="cd_tipo_limite"
		inputType="TEXT"
		inputSize="5"
		maxLength="5"
		label="Tipo limite" />
	<fieldProperty
		name="im_incarichi"
		property="im_incarichi"
		inputType="TEXT"
		formatName="it.cnr.contab.util.EuroFormat"
		enabledOnSearch="true"
		nullable="false"
		label="Importo Incarichi" />
	<fieldProperty
		name="prc_utilizzato"
		property="prc_utilizzato"
		inputType="TEXT"
		inputSize="5"
		maxLength="5"
		formatName="it.cnr.contab.util.PercentFormat"
		label="Percentuale" />
	<fieldProperty
		name="tot_incarichi_assegnati"
		property="tot_incarichi_assegnati"
		inputType="TEXT"
		formatName="it.cnr.contab.util.EuroFormat"
		enabledOnSearch="true"
		nullable="false"
		label="Assegnati" />
	<fieldProperty
		name="tot_incarichi_validi"
		property="tot_incarichi_validi"
		inputType="TEXT"
		formatName="it.cnr.contab.util.EuroFormat"
		enabledOnSearch="true"
		nullable="false"
		label="In Corso" />
	<fieldProperty
		name="tot_incarichi_scaduti"
		property="tot_incarichi_scaduti"
		inputType="TEXT"
		formatName="it.cnr.contab.util.EuroFormat"
		enabledOnSearch="true"
		nullable="false"
		label="Scaduti" />

	<formFieldProperty
		name="esercizio_limite" />
	<formFieldProperty
		name="cd_cds" />
	<formFieldProperty
		name="cd_unita_organizzativa" />
	<formFieldProperty
		name="ds_unita_organizzativa" />
	<formFieldProperty
		name="cd_tipo_limite" />
	<formFieldProperty
		name="im_incarichi" />
	<formFieldProperty
		name="prc_utilizzato" />
	<formFieldProperty
		name="tot_incarichi_assegnati" />
	<formFieldProperty
		name="tot_incarichi_validi" />
	<formFieldProperty
		name="tot_incarichi_scaduti" />

	<findFieldProperty
		name="esercizio_limite" />
	<findFieldProperty
		name="cd_cds" />
	<findFieldProperty
		name="cd_unita_organizzativa" />
	<findFieldProperty
		name="ds_unita_organizzativa" />
	<findFieldProperty
		name="cd_tipo_limite" />
	<findFieldProperty
		name="im_incarichi" />

	<columnFieldProperty
		name="esercizio_limite" />
	<columnFieldProperty
		name="cd_cds" />
	<columnFieldProperty
		name="cd_unita_organizzativa" />
	<columnFieldProperty
		name="ds_unita_organizzativa" />
	<columnFieldProperty
		name="cd_tipo_limite" />
	<columnFieldProperty
		name="im_incarichi" />

	<columnSet
		name="incarichiUo">
		<columnFieldProperty
			name="cd_unita_organizzativa" />
		<columnFieldProperty
			name="ds_unita_organizzativa"
			inputType="TEXT"
			inputSize="50"/>
		<columnFieldProperty
			name="im_incarichi" />
		<columnFieldProperty
			name="prc_utilizzato" />
	</columnSet>
</bulkInfo>