<?xml version="1.0" encoding="UTF-8"?>

<bulkInfo 	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xmlns="https://contab.cnr.it/schema/SIGLA" 
			xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA https://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">
	<fieldProperty
		name="esercizio"
		property="esercizio"
		inputType="TEXT"
		inputSize="4"
		maxLength="4"
		enabledOnInsert="false"
		enabledOnEdit="false"
		enabledOnSearch="true"
		label="Esercizio" />
	<fieldProperty
		name="pg_bonus"
		property="pg_bonus"
		inputType="TEXT"
		inputSize="10"
		maxLength="10"
		enabledOnInsert="false"
		enabledOnEdit="false"
		enabledOnSearch="true"
		label="Progressivo" />
	<fieldProperty
		name="cd_terzo"
		property="cd_terzo"
		enabledOnInsert="false"
		enabledOnEdit="false"
		enabledOnSearch="true"
		inputType="TEXT"
		inputSize="8"
		maxLength="8"
		label="Terzo richiedente" />
	<fieldProperty
		name="esercizio_imposta"
		property="esercizio_imposta"
		enabledOnEdit="false"
		inputType="TEXT"
		inputSize="4"
		maxLength="4"
		label="Esercizio imposta" />
	<fieldProperty
		name="dt_registrazione"
		property="dt_registrazione"
		enabledOnEdit="false"
		inputType="TEXT"
		formatName="date_short"
		label="Data Registrazione" />
	<fieldProperty
		name="codice_fiscale"
		property="codice_fiscale"
		enabledOnEdit="false"
		command="doOnCodice_fiscaleChange"
		formatName="it.cnr.jada.util.UppercaseStringFormat"
		inputType="TEXT"
		enabledOnSearch="true"
		caseSensitiveSearch="false"
		inputSize="20"
		maxLength="20"
		label="Codice Fiscale richiedente" />
	<fieldProperty
		name="cognome"
		property="terzo.anagrafico.cognome"
		inputType="TEXT"
		enabledOnInsert="false"
		enabledOnEdit="false"
		enabledOnSearch="true"
		maxLength="50"
		caseSensitiveSearch="false"
		label="Cognome" />
		
	<fieldProperty
		name="nome"
		property="terzo.anagrafico.nome"
		inputType="TEXT"
		enabledOnSearch="true"
		enabledOnInsert="false"
		enabledOnEdit="false"
		caseSensitiveSearch="false"
		maxLength="50"
		label="Nome" />
	<fieldProperty
		name="ti_sesso"
		property="terzo.anagrafico.ti_sesso"
		enabledOnInsert="false"
		enabledOnEdit="false"
		keysProperty="ti_sessoKeys"
		inputType="RADIOGROUP"
		layout="HORIZONTAL" 
		inputSize="1"
		maxLength="1"
		label="Sesso" />
	<fieldProperty
		name="dt_nascita"
		property="terzo.anagrafico.dt_nascita"
		inputType="ROTEXT"
		formatName="date_short"
		label="Data Nascita" />
