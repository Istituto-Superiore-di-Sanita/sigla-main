<?xml version="1.0" encoding="iso-8859-1"?>
<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="https://contab.cnr.it/schema/SIGLA" 
		xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA https://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">
	<fieldProperty
		name="esercizio"
		property="var_stanz_res.esercizio"
		inputType="TEXT"
		inputSize="4"
		maxLength="4"
		label="Esercizio finaziario" />
	<fieldProperty
		name="pg_variazione"
		property="var_stanz_res.pg_variazione"
		inputType="TEXT"
		inputSize="10"
		maxLength="10"
		label="Progressivo della Variazione" />		
    <fieldProperty
        name="var_stanz_res"
        property="var_stanz_res"
        inputType="SEARCHTOOL"
        formName="searchtool"
        enabledOnInsert="false"
        enabledOnEdit="false"
        enabledOnSearch="true"
        label="Variazione" />
	<fieldProperty
		name="pg_riga"
		property="pg_riga"
		inputType="TEXT"
		inputSize="10"
		maxLength="10"
		label="Progressivo della riga della Variazione" />
	<fieldProperty
		name="esercizio_res"
		property="esercizio_res"
		inputType="TEXT"
		inputSize="4"
		maxLength="4"
		label="Esercizio Residuo" />

	<fieldProperty
		name="cd_cdr"
		property="linea_di_attivita.cd_centro_responsabilita"
		inputType="TEXT"
		inputSize="30"
		maxLength="30"
		label="Codice CDR" />
	<fieldProperty
		name="cd_linea_attivita"
		property="linea_di_attivita.cd_linea_attivita"
		inputType="TEXT"
		enabledOnSearch="true"
		inputSize="10"
		maxLength="10"
		label="Codice G.A.E." />
    <fieldProperty
        name="linea_di_attivita"
        property="linea_di_attivita"
        inputType="SEARCHTOOL"
        formName="searchtool_x_var_stanz_res"
        enabledOnSearch="true"
        label="G.A.E." />
    <fieldProperty
        name="modulo_di_attivita"
        property="linea_di_attivita.progetto"
        inputType="SEARCHTOOL"
        formName="searchtool"
        enabledOnInsert="false"
        enabledOnEdit="false"
        enabledOnSearch="false"
        label="Modulo di Attività" />
        
	<fieldProperty
		name="esercizio_voce"
		property="voce_f.esercizio"
		inputType="TEXT"
		inputSize="4"
		maxLength="4"
		label="Esercizio della Voce" />
	<fieldProperty
		name="ti_appartenenza"
		property="voce_f.ti_appartenenza"
		inputType="TEXT"
		inputSize="1"
		maxLength="1"
		label="Tipo Appartenenza CNR/CDS" />
	<fieldProperty
		name="ti_gestione"
		property="voce_f.ti_gestione"
		inputType="TEXT"
		inputSize="1"
		maxLength="1"
		label="Tipo Gestione Entrata/Spesa" />
	<fieldProperty
		name="cd_voce"
		property="voce_f.cd_voce"
		inputType="TEXT"
		maxLength="50"
		label="Capitolo" />
    <fieldProperty
        name="voce_f"
        property="voce_f"
        inputType="SEARCHTOOL"
        formName="searchtool"
        enabledOnInsert="false"
        enabledOnEdit="false"
        enabledOnSearch="false"
        label="Capitolo" />

    <fieldProperty
        name="elemento_voce"
        property="elemento_voce"
        inputType="SEARCHTOOL"
        formName="searchtool"
        enabledOnSearch="true"
        label="Voce del Piano" />
	<fieldProperty
		name="cd_elemento_voce"
		property="elemento_voce.cd_elemento_voce"
		inputType="TEXT"
		enabledOnSearch="true"
		maxLength="20"
		label="Voce" />
		
	<fieldProperty
		name="im_variazione"
		property="im_variazione"
		inputType="TEXT"
		formatName="it.cnr.contab.util.EuroFormat"
		nullable="false"
		label="Importo" />

	<fieldProperty
		name="disponibilita_stanz_res"
		property="disponibilita_stanz_res"
		inputType="ROTEXT"
		formatName="it.cnr.contab.util.EuroFormat"
		label="Disponibilità stanziamento residuo" />
		
	<formFieldProperty
		name="var_stanz_res" />
	<formFieldProperty
		name="voce_f" />
	<formFieldProperty
		name="elemento_voce" />
	<formFieldProperty
		name="esercizio_res" />
	<formFieldProperty
		name="linea_di_attivita" />
	<formFieldProperty
		name="modulo_di_attivita" />		
	<formFieldProperty
		name="im_variazione" />
	<formFieldProperty
		name="disponibilita_stanz_res" />

	<findFieldProperty
		name="esercizio" />
	<findFieldProperty
		name="pg_variazione" />
	<findFieldProperty
		name="pg_riga" />
	<findFieldProperty
		name="esercizio_voce" />
	<findFieldProperty
		name="esercizio_res" />
	<findFieldProperty
		name="cd_cdr" />
	<findFieldProperty
		name="cd_linea_attivita" />
	<findFieldProperty
		name="ti_appartenenza" />
	<findFieldProperty
		name="ti_gestione" />
	<findFieldProperty
		name="cd_voce" />
	<findFieldProperty
		name="im_variazione" />
		
	<columnFieldProperty
		name="esercizio" />
	<columnFieldProperty
		name="pg_variazione" />
	<columnFieldProperty
		name="pg_riga" />
	<columnFieldProperty
		name="esercizio_voce" />
	<columnFieldProperty
		name="esercizio_res" />
	<columnFieldProperty
		name="cd_cdr" />
	<columnFieldProperty
		name="cd_linea_attivita" />
	<columnFieldProperty
		name="ti_appartenenza" />
	<columnFieldProperty
		name="ti_gestione" />
	<columnFieldProperty
		name="cd_voce" />
	<columnFieldProperty
		name="im_variazione" />
	<columnSet
	        name="rigaVariazione">
        	<columnFieldProperty
        		name="cd_linea_attivita" />
        	<columnFieldProperty
        		name="cd_elemento_voce" />
        	<columnFieldProperty
        		name="im_variazione" />        		
	</columnSet>		
</bulkInfo>