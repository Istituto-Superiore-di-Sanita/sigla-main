<?xml version="1.0" encoding="iso-8859-1"?>
<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="https://contab.cnr.it/schema/SIGLA" 
		xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA https://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">
	<fieldProperty
		name="esercizio"
		property="esercizio"
		inputType="TEXT"
		inputSize="4"
		maxLength="4"
		label="Esercizio finanziario" />
	<fieldProperty
		name="pg_variazione"
		property="pg_variazione"
		inputType="TEXT"
		inputSize="10"
		maxLength="10"
		label="Progressivo della variazione (unico per Ente)" />
    <fieldProperty
        name="centro_di_responsabilita"
        property="centro_di_responsabilita"
        inputType="SEARCHTOOL"
        formName="searchtool"
        enabledOnInsert="true"
        enabledOnEdit="true"
        enabledOnSearch="true"
		nullable="false"
        label="Centro di Responsabilità" />		                
	<fieldProperty
		name="cd_centro_responsabilita"
		property="centro_di_responsabilita.cd_centro_responsabilita"
		inputType="TEXT"
		inputSize="30"
		maxLength="30"
		label="Centro di Responsabilità" />
	<fieldProperty
		name="ds_centro_responsabilita"
		property="centro_di_responsabilita.ds_cdr"
		inputType="TEXT"
		inputSize="50"
		maxLength="50"
		nullable="false"
		label="Descrizione" />
	<fieldProperty
		name="im_spesa"
		property="im_spesa"
		inputType="TEXT"
		formatName="it.cnr.contab.util.EuroFormat"
		nullable="false"
		command="doValidaSpesa"
		label="Quota di spesa assegnata" />
	<fieldProperty
		name="spesa_ripartita"
		property="spesa_ripartita"
		inputType="ROTEXT"
		formatName="it.cnr.contab.util.EuroFormat"
		label="Quota di spesa ripartita" />		
	<fieldProperty
		name="spesa_diff"
		property="spesa_diff"
		inputType="ROTEXT"
		style="text-align: right"
		formatName="it.cnr.contab.util.EuroFormat"
		label="Differenza" />		
	<fieldProperty
		name="dettagliSpese"
		inputType="BUTTON"
		img="img/edit16.gif"
		style="align: center;"
		label="Dettagli Spese"
		iconClass="fa fa-fw fa-briefcase"
		buttonClass="btn-outline-primary btn-title"
		readonlyProperty="new"
		readonlyPropertyOnView="new"
		enabledOnView="true"
		href="javascript:submitForm('doDettagliSpe')" />		

	<formFieldProperty
		name="esercizio" />
	<formFieldProperty
		name="pg_variazione" />
	<formFieldProperty
		name="centro_di_responsabilita" />
	<formFieldProperty
		name="im_spesa" />
	<formFieldProperty
		name="spesa_ripartita" />
	<formFieldProperty
		name="spesa_diff" />
	<formFieldProperty
		name="dettagliSpese" />
		
		
	<findFieldProperty
		name="esercizio" />
	<findFieldProperty
		name="pg_variazione" />
	<findFieldProperty
		name="cd_centro_responsabilita" />
	<findFieldProperty
		name="im_spesa" />
	<columnFieldProperty
		name="esercizio" />
	<columnFieldProperty
		name="pg_variazione" />
	<columnFieldProperty
		name="cd_centro_responsabilita" />
	<columnFieldProperty
		name="im_spesa" />
	<columnSet
	        name="associazioneCDR">
        	<columnFieldProperty
        		name="cd_centro_responsabilita" />
        	<columnFieldProperty
        		name="ds_centro_responsabilita" />
        	<columnFieldProperty
        		name="im_spesa" />        		
	</columnSet>		
</bulkInfo>