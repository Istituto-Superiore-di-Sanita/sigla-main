<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="http://contab.cnr.it/schema/SIGLA" 
		xsi:schemaLocation="http://contab.cnr.it/schema/SIGLA http://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">

	<fieldProperty
		name="cd_natura"
		property="cd_natura"
		label="Natura"
		inputType="TEXT"
		/>
	<fieldProperty
		name="ds_natura"
		property="ds_natura"
		label="descrizione"
