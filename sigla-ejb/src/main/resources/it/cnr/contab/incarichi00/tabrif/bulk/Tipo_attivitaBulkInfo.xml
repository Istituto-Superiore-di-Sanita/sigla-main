		name="ds_tipo_attivita" />
	<formFieldProperty
		name="fl_pubblica_contratto" />
	<formFieldProperty
		name="fl_cancellato" />

	<columnFieldProperty
		name="cd_tipo_attivita" />
	<columnFieldProperty
		name="ds_tipo_attivita" />
	<columnFieldProperty
		name="fl_pubblica_contratto" />
	<columnFieldProperty
		name="fl_cancellato" />

	<findFieldProperty
		name="cd_tipo_attivita" />
	<findFieldProperty
		name="ds_tipo_attivita" />
	<findFieldProperty
		name="fl_pubblica_contratto" />
	<findFieldProperty
		name="fl_cancellato" />

	<form
		name="searchtool" >
		<formFieldProperty
			name="cd_tipo_attivita" />
		<formFieldProperty
			name="ds_tipo_attivita"
			inputType="TEXT"
			inputSize="30" />
	</form>
</bulkInfo>
