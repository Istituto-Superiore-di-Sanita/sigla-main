		name="uo" />
	
	
	<findFieldProperty
		name="chiave" />
	<findFieldProperty
		name="tipo" />
	<findFieldProperty
		name="sequenza" />
	<findFieldProperty
		name="esercizio" />
	<findFieldProperty
		name="cd_cds" />
	<findFieldProperty
		name="uo" />
	
	
	<columnFieldProperty
		name="chiave" />
	<columnFieldProperty
		name="tipo" />
	<columnFieldProperty
		name="sequenza" />
	<columnFieldProperty
		name="esercizio" />
	<columnFieldProperty
		name="cd_cds" />
	<columnFieldProperty
		name="uo" />
	
</bulkInfo>