<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="https://contab.cnr.it/schema/SIGLA" 
		xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA https://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">

	<fieldProperty
		name="cd_ruolo"
		property="ruolo.cd_ruolo"
		label="Ruolo"
		inputType="TEXT"
		/>
	<fieldProperty
		name="esercizio"
		property="esercizio"
		label="Esercizio"
		inputType="TEXT"
		/>
	<fieldProperty
		name="fl_attiva"
		property="fl_attiva"
		inputType="CHECKBOX"
		inputSize="1"
		maxLength="1"
		enabledOnEdit="true"
