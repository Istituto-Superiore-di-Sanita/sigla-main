	<formFieldProperty
		name="cd_unita_organizzativa" />
	<formFieldProperty
		name="pg_progetto" />
	<formFieldProperty
		name="unita_organizzativa" />
	<formFieldProperty
		name="importo" />

	<columnFieldProperty
		name="cd_unita_organizzativa" />
	<columnFieldProperty
		name="pg_progetto" />
	<columnFieldProperty
		name="importo" />

	<findFieldProperty
		name="cd_unita_organizzativa" />
