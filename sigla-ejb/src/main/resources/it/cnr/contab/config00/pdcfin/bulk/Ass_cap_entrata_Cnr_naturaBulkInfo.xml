		inputType="TEXT"
		maxLength="20"
		inputSize="10"
		enabledOnSearch="true"
		/>
	<fieldProperty
		name="ds_elemento_voce"
		property="elemento_voce.ds_elemento_voce"
		readonlyProperty="ROElemento_voce"
