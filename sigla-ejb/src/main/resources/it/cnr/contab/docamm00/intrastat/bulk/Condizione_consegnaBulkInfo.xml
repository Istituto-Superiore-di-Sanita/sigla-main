		name="ds_condizione_consegna" />

	<columnFieldProperty
		name="esercizio" />
	<columnFieldProperty
		name="cd_incoterm" />
	<columnFieldProperty
		name="cd_gruppo" />
	<columnFieldProperty
		name="ds_condizione_consegna" />		
	
	<findFieldProperty
		name="esercizio" />
	<findFieldProperty
		name="cd_incoterm" />
	<findFieldProperty
		name="cd_gruppo" />
	<findFieldProperty
		name="ds_condizione_consegna" />
</bulkInfo>