	<findFieldProperty
		name="box_comunicazioni" />
	<findFieldProperty
		name="box_scadenze" />		
	<findFieldProperty
		name="fl_autenticazione_ldap" />
	<findFieldProperty
		name="ldap_user" />						
	<findFieldProperty
		name="ldap_password" />						
	<findFieldProperty
		name="ldap_base_dn" />						
