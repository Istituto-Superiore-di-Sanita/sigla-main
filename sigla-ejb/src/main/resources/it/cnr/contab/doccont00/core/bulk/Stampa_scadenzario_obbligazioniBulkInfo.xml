<?xml version="1.0" encoding="iso-8859-1"?>
<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="http://contab.cnr.it/schema/SIGLA" 
		xsi:schemaLocation="http://contab.cnr.it/schema/SIGLA http://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">

	<fieldProperty
		name="dataInizio"
		property="dataInizio"
		inputType="TEXT"
		formatName="date_short"
		maxLength="10"
		inputSize="10"
		nullable="false"
		label="Data inizio"/>
	<fieldProperty
		name="dataFine"
		property="dataFine"
		inputType="TEXT"
		formatName="date_short"
		maxLength="10"
		inputSize="10"
		nullable="false"
		label="Data fine" />
	<fieldProperty
		name="findUoForPrint"
		property="uoForPrint"
		inputType="SEARCHTOOL"
		readonlyProperty="ROUoForPrintSearchTool"
		completeOnSave = "true"
		label="UO emittente" />
	<fieldProperty
		name="cdUoForPrint"
		property="uoForPrint.cd_unita_organizzativa"
		maxLength="30"
		inputSize="20"
		inputType="TEXT"
		nullable="true"
		readonlyProperty="ROUoForPrint"
		label="Codice UO" />
	<fieldProperty
		name="dsUoForPrint"
		property="uoForPrint.ds_unita_organizzativa"
		maxLength="80"
		inputSize="60"
		inputType="ROTEXT"
		label="Descrizione UO" />
		
	<formFieldProperty
		name="esercizio"
		inputType="ROTEXT" />
	<formFieldProperty
		name="cd_cds"
		inputType="ROTEXT" />
	<formFieldProperty name="dataInizio" />
	<formFieldProperty name="dataFine" />
	<formFieldProperty name="findUoForPrint" />
	<formFieldProperty name="cdUoForPrint" />
	<formFieldProperty name="dsUoForPrint" />

	<form
		name="/cnrdoccont/doccont/scadobblig.rpt">
		<formFieldProperty
			name="esercizio"
			inputType="ROTEXT" />
		<formFieldProperty 
			name="cd_cds"
			inputType="ROTEXT" />
		<formFieldProperty name="dataInizio" />
		<formFieldProperty name="dataFine" />
		<formFieldProperty name="findUoForPrint" />
		<formFieldProperty name="cdUoForPrint" />
		<formFieldProperty name="dsUoForPrint" />
	</form>

	<form
		name="/cnrdoccont/doccont/scadobblig.jasper">
		<formFieldProperty
			name="esercizio"
			inputType="ROTEXT" />
		<formFieldProperty 
			name="cd_cds"
			inputType="ROTEXT" />
		<formFieldProperty name="dataInizio" />
		<formFieldProperty name="dataFine" />
		<formFieldProperty name="findUoForPrint" />
		<formFieldProperty name="cdUoForPrint" />
		<formFieldProperty name="dsUoForPrint" />
	</form>
	
	
	<printForm
		name="/cnrdoccont/doccont/scadobblig.rpt">
		<printFieldProperty 
			name="esercizio"
			parameterPosition="0"
			parameterType="STRING" />
		<printFieldProperty 
			name="cdUoForPrint"
			property="cdUoNullableForPrint"
			parameterPosition="1"
			parameterType="STRING" />
		<printFieldProperty 
			name="dataInizio"
			parameterPosition="2"
			parameterType="DATE" />
		<printFieldProperty 
			name="dataFine"
			parameterPosition="3"
			parameterType="DATE" />
		<printFieldProperty 
			name="cd_cds"
			parameterPosition="4"
			parameterType="STRING" />
	</printForm>
	
	<printForm
		name="/cnrdoccont/doccont/scadobblig.jasper">
		<printFieldProperty 
			name="esercizio"
			parameterPosition="0"
			parameterType="STRING" 
			paramNameJR="Esercizio"
			paramTypeJR="java.lang.Integer" />
		<printFieldProperty 
			name="cdUoForPrint"
			property="cdUoNullableForPrint"
			parameterPosition="1"
			parameterType="STRING" 
			paramNameJR="UO"
			paramTypeJR="java.lang.String"/>
		<printFieldProperty 
			name="dataInizio"
			parameterPosition="2"
			parameterType="DATE" 
			paramNameJR="DataInizio"
			paramTypeJR="java.util.Date"/>
		<printFieldProperty 
			name="dataFine"
			parameterPosition="3"
			parameterType="DATE"
			paramNameJR="DataFine"
			paramTypeJR="java.util.Date" />
		<printFieldProperty 
			name="cd_cds"
			parameterPosition="4"
			parameterType="STRING" 
			paramNameJR="CDS"
			paramTypeJR="java.lang.String"/>
	</printForm>
	
	<printForm
		name="/cnrdoccont/doccont/obbligazioni_riportabili.jasper">
		<printFieldProperty 
			name="esercizio"
			parameterPosition="0"
			parameterType="STRING" 
			paramNameJR="Esercizio"
			paramTypeJR="java.lang.Integer" />
		<printFieldProperty 
			name="cdUOCRForPrint"
			parameterPosition="2"
			parameterType="STRING" 
			paramNameJR="UO"
			paramTypeJR="java.lang.String" />
	</printForm>	

</bulkInfo>