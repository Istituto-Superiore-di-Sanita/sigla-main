<?xml version="1.0" encoding="UTF-8"?>
<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="http://contab.cnr.it/schema/SIGLA" 
		xsi:schemaLocation="http://contab.cnr.it/schema/SIGLA http://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">
	<fieldProperty
		name="cd_tipo_prestazione"
		property="cd_tipo_prestazione"
		inputType="TEXT"
		inputSize="5"
		maxLength="5"
		enabledOnSearch="true"
		nullable="false"
		label="Codice" />
	<fieldProperty
		name="ds_tipo_prestazione"
		property="ds_tipo_prestazione"
		inputType="TEXTAREA"
		maxLength="200"
		cols="60"
		rows="5"
		enabledOnSearch="true"
		nullable="false"
		label="Descrizione" />
	<fieldProperty
		name="tipo_classificazione"
		property="tipo_classificazione"
		inputSize="100"
		maxLength="100"
		nullable="false"
		optionsProperty="tipologie_variazione"
		inputType="SELECT"		
		label="Classificazione" />
	<fieldProperty
		name="fl_cancellato"
		property="fl_cancellato"
		inputType="CHECKBOX"
		enabledOnSearch="true"
		enabledOnInsert="false"
		enabledOnEdit="false"
		nullable="false"
		label="Eliminato" />

	<formFieldProperty
		name="cd_tipo_prestazione" />
