		name="cdEsercizioDocForPrint"
		property="esercizioDocForPrint.esercizio" 
		maxLength="4"
		inputSize="2"
		readonlyProperty="ROEsercizioDocForPrint"
		inputType="TEXT"/>
 -->
	<formFieldProperty
		name="esercizio" />
	<formFieldProperty
		name="findCdsForPrint" />
	<formFieldProperty
		name="cdCdsForPrint" />
	<formFieldProperty
		name="dsCdsForPrint" />
	<formFieldProperty
		name="findUoForPrint" />
	<formFieldProperty
		name="cdUoForPrint" />
	<formFieldProperty
		name="dsUoForPrint" />
	<formFieldProperty
		name="findElementoVoceForPrint" />
	<formFieldProperty
		name="cdElementoVoceForPrint" />
	<formFieldProperty
		name="dsElementoVoceForPrint" />
	<formFieldProperty
		name="findEsercizioDocForPrint" />
	<formFieldProperty
		name="cdEsercizioDocForPrint" />

	<printForm
		name="/cnrdoccont/doccont/associazioneobb_docamm.rpt" >
		<printFieldProperty
			name="esercizio"
			parameterPosition="0"
			parameterType="STRING" />
		<printFieldProperty
			name="cdCdsForPrint"
			parameterPosition="1"
			parameterType="STRING" />
		<printFieldProperty
			name="cdUoForPrint"
			parameterPosition="2"
			parameterType="STRING" />
		<printFieldProperty
			name="cdElementoVoceForPrint"
			parameterPosition="3"
			parameterType="STRING" />
		<printFieldProperty
			name="cdEsercizioDocForPrint"
			parameterPosition="4"
			parameterType="STRING" />
	</printForm>
</bulkInfo>
