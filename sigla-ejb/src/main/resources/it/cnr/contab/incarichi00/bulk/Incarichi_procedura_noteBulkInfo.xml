<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="https://contab.cnr.it/schema/SIGLA" 
		xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA https://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">
	<fieldProperty
		name="esercizio"
		property="incarichi_procedura.esercizio"
		inputType="TEXT"
		inputSize="4"
		maxLength="4"
		enabledOnSearch="true"
		enabledOnInsert="false"
		enabledOnEdit="false"
		nullable="false"
		label="Anno" />
	<fieldProperty
		name="pg_procedura"
		property="incarichi_procedura.pg_procedura"
		inputType="TEXT"
		inputSize="10"
		maxLength="10"
		enabledOnSearch="true"
		enabledOnInsert="false"
		enabledOnEdit="false"
		nullable="false"
		label="Identificativo Ente" />
	<fieldProperty
		name="pg_nota"
		property="pg_nota"
		inputType="TEXT"
		inputSize="4"
		maxLength="4"
		enabledOnSearch="true"
		enabledOnInsert="false"
		enabledOnEdit="false"
		nullable="false"
		label="Progressivo nota" />
	<fieldProperty
		name="nota"
		property="nota"
		inputType="TEXTAREA"
		maxLength="1000"
		cols="90"
		rows="6"
		enabledOnSearch="true"
		enabledOnInsert="true"
		enabledOnEdit="true"
		nullable="false"
		label="Annotazione" />
