		formName="searchtool"
		enabledOnSearch="true"
		label="Ruolo" />

	<columnFieldProperty
		name="cd_ruolo"
		/>
	<columnFieldProperty
		name="esercizio"
		/>
	<columnFieldProperty
		name="fl_attiva"
		/>

	<formFieldProperty
		name="cd_ruolo"
		/>
	<formFieldProperty
		name="esercizio"
		/>
	<formFieldProperty
		name="ruolo"
		/>
	<formFieldProperty
		name="fl_attiva"
		/>

	<findFieldProperty
		name="cd_accesso"
		/>
