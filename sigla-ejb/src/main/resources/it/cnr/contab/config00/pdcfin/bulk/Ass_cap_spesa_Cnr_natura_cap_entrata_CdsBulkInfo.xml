<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="http://contab.cnr.it/schema/SIGLA" 
		xsi:schemaLocation="http://contab.cnr.it/schema/SIGLA http://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">

	<fieldProperty
		name="cd_natura"
		property="cd_natura"
		label="Natura"
		keysProperty="naturaKeys"
		inputType="SELECT"
		/>
	<fieldProperty
		name="find_elemento_voce"
		property="elemento_voce"
		inputType="SEARCHTOOL"
		enabledOnSearch="true"
		/>
	<fieldProperty
		name="cd_elemento_voce"
		property="elemento_voce.cd_elemento_voce"
		readonlyProperty="ROElemento_voce"
		label="Titolo di Spesa CNR"
		inputType="TEXT"
		maxLength="20"
		inputSize="10"
		enabledOnSearch="true"
		/>
	<fieldProperty
		name="ds_elemento_voce"
		property="elemento_voce.ds_elemento_voce"
		readonlyProperty="ROElemento_voce"
		label=""
		inputType="ROTEXT"
		inputSize="20"
		enabledOnSearch="true"
		/>


	<fieldProperty
		name="find_tipo_unita"
		property="tipo_unita"
		inputType="SEARCHTOOL"
		enabledOnSearch="true"
		/>
	<fieldProperty
		name="cd_tipo_unita"
		property="tipo_unita.cd_tipo_unita"
		label="Tipo di CDS"
		readonlyProperty="ROTipo_unita"
		inputType="TEXT"
		maxLength="20"
		inputSize="10"
		enabledOnSearch="true"
		/>
	<fieldProperty
		name="ds_tipo_unita"
		property="tipo_unita.ds_tipo_unita"
		readonlyProperty="ROTipo_unita"
		label=""
		inputType="ROTEXT"
		inputSize="20"
		enabledOnSearch="true"
		/>

	<fieldProperty
		name="find_elemento_voce_coll"
		property="elemento_voce_coll"
		inputType="SEARCHTOOL"
		enabledOnSearch="true"
		/>
	<fieldProperty
		name="cd_elemento_voce_coll"
		property="elemento_voce_coll.cd_elemento_voce"
		readonlyProperty="ROElemento_voce_coll"
		label="Capitolo Entrata CDS"
		inputType="TEXT"
		maxLength="20"
		inputSize="10"
		enabledOnSearch="true"
		/>
	<fieldProperty
		name="ds_elemento_voce_coll"
		property="elemento_voce_coll.ds_elemento_voce"
		readonlyProperty="ROElemento_voce_coll"
		label=""
		inputType="ROTEXT"
		inputSize="20"
		enabledOnSearch="true"
		/>
		
	<columnFieldProperty
		name="cd_natura"
		/>
	<columnFieldProperty
		name="cd_elemento_voce"
		/>

	<columnFieldProperty
		name="cd_tipo_unita"
		/>

	<columnFieldProperty
		name="cd_elemento_voce_coll"
		/>

	<formFieldProperty
		name="cd_natura"
		/>
	<formFieldProperty
		name="find_elemento_voce"
		/>
	<formFieldProperty
		name="cd_elemento_voce"
		/>
	<formFieldProperty
		name="ds_elemento_voce"
		/>


	<formFieldProperty
		name="find_tipo_unita"
		/>
	<formFieldProperty
		name="cd_tipo_unita"
		/>
	<formFieldProperty
		name="ds_tipo_unita"
		/>


	<formFieldProperty
		name="find_elemento_voce_coll"
		/>
	<formFieldProperty
		name="cd_elemento_voce_coll"
		/>
	<formFieldProperty
		name="ds_elemento_voce_coll"
		/>

	<findFieldProperty
		name="cd_natura"
		/>
	<findFieldProperty
		name="cd_elemento_voce"
		/>

	<findFieldProperty
		name="cd_tipo_unita"
		/>

	<findFieldProperty
		name="cd_elemento_voce_coll"
		/>
	
</bulkInfo>