<?xml version="1.0" encoding="UTF-8"?>
<bulkInfo
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://contab.cnr.it/schema/SIGLA"
        xsi:schemaLocation="http://contab.cnr.it/schema/SIGLA http://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd"
        shortDescription="Associazione bisess process con accesso"
        longDescription="Associazione bisess process con accesso">
    <fieldProperty
            name="cdAccesso"
            property="cdAccesso"
            inputType="TEXT"
            inputSize="20"
            maxLength="20"
            enabledOnEdit="true"
            enabledOnSearch="true"
            enabledOnFreeSearch="true"
            nullable="false"
            label="Codice"/>
    <fieldProperty
            name="dsAccesso"
            property="accesso.ds_accesso"
            inputType="TEXT"
            inputSize="50"
            maxLength="250"
            enabledOnEdit="true"
            enabledOnSearch="true"
            nullable="false"
            label="Descrizione"/>
