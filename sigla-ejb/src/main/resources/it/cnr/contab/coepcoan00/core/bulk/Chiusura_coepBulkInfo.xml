<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="https://contab.cnr.it/schema/SIGLA" 
		xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA https://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">
	<fieldProperty
		name="cd_cds"
		property="cd_cds"
		inputType="TEXT"
		inputSize="30"
		maxLength="30"
		label="Codice cds" />
	<fieldProperty
		name="esercizio"
		property="esercizio"
		inputType="TEXT"
		inputSize="4"
		maxLength="4"
		label="Esercizio contbile" />
	<fieldProperty
                name="statoText"
                property="stato"
                inputType="TEXT"
                keysProperty="ti_statoKeys"
                nullable="false"
                enabledOnSearch="true"
                enabledOnInsert="false"
                enabledOnEdit="false"                
                label="Stato" />
                
	<formFieldProperty
		name="cd_cds" />
	<formFieldProperty
		name="esercizio" />
	<formFieldProperty
		name="stato" />
	<findFieldProperty
		name="cd_cds" />
	<findFieldProperty
		name="esercizio" />
	<findFieldProperty
		name="stato" />
	<columnFieldProperty
		name="cd_cds" />
	<columnFieldProperty
		name="esercizio" />
	<columnFieldProperty
		name="stato" />
</bulkInfo>