<?xml version="1.0" encoding="UTF-8"?>
<bulkInfo
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="https://contab.cnr.it/schema/SIGLA" 
		xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA https://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd">
	<fieldProperty
		name="esercizio"
		property="esercizio"
		inputType="TEXT"
		inputSize="4"
		maxLength="4"
		enabledOnEdit="false"
		enabledOnInsert="false"
		enabledOnSearch="false"
		enabledOnFreeSearch="false" 
		label="Esercizio" />
	<fieldProperty
		name="ti_gestione"
		property="ti_gestione"
		inputSize="1"
		maxLength="1"
		inputType="TEXT"
		label="Gestione" />
	<fieldProperty
		name="cd_siope"
		property="cd_siope"
		inputType="TEXT"
		inputSize="10"
		maxLength="10"
		label="Codice Siope" />
	<fieldProperty
		name="descrizione"
		property="descrizione"
		inputType="TEXTAREA"
		cols="60"
		rows="5"
		maxLength="200"
		label="Descrizione" />
	
		
	<form
		name="searchtool" >
		<formFieldProperty
			name="cd_siope"
			enabledOnInsert = "true"
			enabledOnEdit = "true"
			enabledOnSearch = "true"
			enabledOnFreeSearch = "true"
			inputType="TEXT"
			inputSize="4" />
		<formFieldProperty
			name="descrizione"
			enabledOnInsert = "true"
			enabledOnEdit = "true"
			enabledOnSearch = "true"
			enabledOnFreeSearch = "true"
			inputType="TEXT"
			inputSize="80" />
	</form>
	
	<formFieldProperty
		name="esercizio" />
	<formFieldProperty
		name="ti_gestione" />
	<formFieldProperty
		name="cd_siope" />
	<formFieldProperty
		name="descrizione" />
	

	<findFieldProperty
		name="esercizio" />
	<findFieldProperty
		name="ti_gestione" />
	<findFieldProperty
		name="cd_siope" />
	<findFieldProperty
		name="descrizione" />
	
		
	<columnFieldProperty
		name="esercizio" />
	<columnFieldProperty
		name="ti_gestione" />
	<columnFieldProperty
		name="cd_siope" />
	<columnFieldProperty
		name="descrizione" />
	
	<columnSet name="collegaARigaDocCont">
		<columnFieldProperty
			name="cd_siope" />
		<columnFieldProperty
			name="descrizione" />
	</columnSet>
</bulkInfo>