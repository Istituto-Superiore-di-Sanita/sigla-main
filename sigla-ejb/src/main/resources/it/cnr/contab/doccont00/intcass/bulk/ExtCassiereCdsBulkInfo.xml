<bulkInfo
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="https://contab.cnr.it/schema/SIGLA"
	xsi:schemaLocation="https://contab.cnr.it/schema/SIGLA http://contab.cnr.it/schema/SIGLA/BulkInfo_1_5.xsd"
	shortDescription="Configurazione interfaccia cassiere"
